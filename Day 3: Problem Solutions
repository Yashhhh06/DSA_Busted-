//Day 3: Problem Solutions

// Problem 1: Factorial
#include <iostream>
using namespace std;

long long factorial(int n) {
    if (n == 0 || n == 1) return 1;
    return n * factorial(n - 1);
}

int main() {
    int T, N;
    cin >> T;
    while (T--) {
        cin >> N;
        cout << factorial(N) << endl;
    }
    return 0;
}

// Problem 2: Multiplication Table
#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;
    for (int i = 1; i <= 12; i++) {
        cout << N << " * " << i << " = " << N * i << endl;
    }
    return 0;
}

// Problem 3: Print 1 to N
#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;
    for (int i = 1; i <= N; i++) {
        cout << i << endl;
    }
    return 0;
}
